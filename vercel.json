{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/public/build/manifest.json"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/build/index.html"
    }
  ]
}
